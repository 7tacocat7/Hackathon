{{#partial "content"}}

    <h1>Post list!</h1>

    {{#if team}}
        {{#each team }}
            <p>team name:</p>
            <p>{{ teamName }}</p>
            <h5>Description:</h5>
            <p> {{description }}</p>
        {{/each}}
    {{else}}
        <p>No teams!</p>
    {{/if}}
{{/partial}}

{{> layout.hbs}}