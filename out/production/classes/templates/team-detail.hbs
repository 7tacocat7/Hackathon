{{#partial "content"}}
    <p>Team name:</p>
    <p>{{ team.teamName }}</p>
    <h5>Description:</h5>
    <p> {{ team.description }}</p>
    <h5>Post status:</h5>
    <p>
        {{#if team.name }}
            <span class="glyphicon glyphicon-star" aria-hidden="true"></span>  Published
        {{else}}
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> Draft
        {{/if}}
    </p>
{{/partial}}

{{> layout.hbs}}