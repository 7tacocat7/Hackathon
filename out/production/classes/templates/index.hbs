{{#partial "content"}}

    <h1>Team list!</h1>

    {{#if team}}
        {{#each team }}
            <p>Current team:</p>
            <p>{{ teamName }} <a href="/teams/{{teamId}}">read more...</a></p>
        {{/each}}
    {{else}}
        <p>No teams!</p>
    {{/if}}
{{/partial}}

{{> layout.hbs}}