{{#partial "content"}}


    <h1>Team list!</h1>

    {{#if teams}}
        {{#each teams }}
            <p>Current team:</p>
            <p>{{ teamName }} <br>{{ description }}<br><a href="/members/{{memberId}}/teams/{{id}}">read more...</a></p>
        {{/each}}
    {{else}}
        <p>No teams!</p>
    {{/if}}
{{/partial}}

{{> layout.hbs}}